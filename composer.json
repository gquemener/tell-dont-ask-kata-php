  "type": "project",
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "friendsofphp/php-cs-fixer": "^3.57"
  },
  "license": "GNU GENERAL PUBLIC LICENSE",
  "autoload": {
    "psr-4": {
      "Pitchart\\TellDontAskKata\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Pitchart\\TellDontAskKata\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Julien VITTE",
      "email": "julien.vitte@insidegroup.fr"
    }
  ],
